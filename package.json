  "devDependencies": {
    "babel-core": "^6.3.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-runtime": "^5.8.0",
    "babelify": "^7.2.0",
    "beefy": "^2.1.5",
