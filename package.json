{
  "name": "Kurtography",
  "version": "0.13.0",
  "description": "Cartography application supported by Kuzzle",
  "main": "app.js",
  "scripts": {
    "test": "beefy app-map.js:bundle.js --live",
    "build-map": "browserify -e app-map.js -o ./public/js/bundle.js",
    "build-css": "cleancss ./dist/css/ol3-drawcontrols.css ./dist/css/style.css -o ./public/css/bundle.css",
    "start": "node app.js"
  },
  "dependencies": {
    "awesomplete": "^1.1.0",
    "clean-css": "^3.4.12",
    "dateformat": "^1.0.12",
    "dialog-polyfill": "^0.4.3",
    "express": "^4.13.4",
    "express-favicon": "^1.0.1",
    "jade": "^1.11.0",
    "kuzzle-sdk": "^2.0.1",
    "node-uuid": "^1.4.7",
    "ogr2ogr": "^0.5.1",
    "openlayers": "^3.15.1",
    "turf-centroid": "^1.1.3",
    "turf-inside": "^1.1.4"
  },
  "devDependencies": {
    "babel-core": "^6.3.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-runtime": "^5.8.0",
    "babelify": "^7.2.0",
    "beefy": "^2.1.5",
    "browserify": "^13.0.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HamHamFonFon/kurtogaphy.git"
  },
  "keywords": [
    "kuzzle",
    "openlayers",
    "nodeJS",
    "browserfify",
    "babel",
    "ES6"
  ],
  "author": "St√©phane MEAUDRE <smeaudre@kaliop.com> (S. Meaudre)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HamHamFonFon/kurtogaphy/issues"
  }
}
